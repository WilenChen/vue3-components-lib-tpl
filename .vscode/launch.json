    {
      "type": "node",
      "request": "launch",
      "name": "Proxy",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:proxy"],
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
